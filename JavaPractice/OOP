package oop;


public class Oop {

  
    public static void main(String[] args) {
        
    Person p = new Person();
    p.firstname = "David";
    p.lastname = "Valerio";
    p.sex = 'M';
    p.age = 19;
    
     Person p1 = new Person();
    p1.firstname = "Alimer";
    p1.lastname = "kemeti";
    p1.sex = 'M';
    p1.age = 19;
    
    System.out.println(p.firstname);
    System.out.println(p.lastname);
    }
    
}
